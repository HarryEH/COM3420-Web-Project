- content_for :title, "Edit Moderators"

.row
  .col-md-6
    %h2 Activate / Deactivate Moderator Account
    %h3 Enter the moderator's email below.
    = form_for(@alteredMod) do |f|
      = f.label :email
      = f.text_field :email, class: "form-control"
      = f.submit "Update", class: "btn btn-default"

.row
  .col-md-6
    %br
    %br
    %br
    %a.btn.btn-primary{:href => "/modpanel", :role => "button"} Back

